{
  "name": "@flower/client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:prod": "NODE_ENV=production next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit --skipLibCheck",
    "type-check:watch": "tsc --noEmit --skipLibCheck --watch",
    "analyze": "ANALYZE=true next build",
    "clean": "rm -rf .next out node_modules",
    "clean:cache": "rm -rf .next",
    "fix:imports": "node scripts/fix-all-imports.js",
    "check:types": "node scripts/check-types-simple.js",
    "find:console": "node scripts/find-console.js",
    "clean:console": "node scripts/clean-console-safe.js",
    "init:storage": "tsx scripts/init-storage-direct.ts",
    "check:storage": "tsx scripts/check-storage.ts",
    "test:integration": "tsx scripts/test-integration.ts",
    "pre-deploy": "pnpm run type-check && pnpm run lint && pnpm run build:prod",
    "deploy:check": "node scripts/deployment-check.js"
  },
  "dependencies": {
    "@flower/shared": "workspace:*",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.51.0",
    "@tanstack/react-query": "^5.83.0",
    "dayjs": "^1.11.13",
    "lucide-react": "^0.525.0",
    "next": "14.2.22",
    "next-pwa": "5.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.60.0",
    "react-hot-toast": "^2.5.2",
    "uuid": "^11.1.0",
    "workbox-webpack-plugin": "^7.3.0",
    "workbox-window": "^7.3.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.22",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.4",
    "typescript": "^5.5.3"
  }
}